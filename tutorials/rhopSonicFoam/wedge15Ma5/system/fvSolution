solvers
{
    p               ICCG 1e-15 0;
    rho             BICCG 1e-15 0;
    rhoU            BICCG 1e-15 0;
    rhoE            BICCG 1e-15 0;
    rhoH            BICCG 1e-15 0;
}

PISO
{
    nOuterCorrectors 2;
    nCorrectors      2;
    HbyAblend        0.95;
}



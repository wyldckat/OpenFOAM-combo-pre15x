FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U     (0 0 0)
    volScalarFieldValue rho   1.0
    volScalarFieldValue T     348.43206
    volScalarFieldValue p     1.0e+5
    volScalarFieldValue rhoE  0.0
    volVectorFieldValue rhoU  (0 0 0)
);

regions
(
    boxToCell
    {
        box    (0 -1 -1) (5 1 1);

        fieldValues
        (
            volScalarFieldValue rho   0.125
            volScalarFieldValue T     278.74564
            volScalarFieldValue p     1.0e+4
        );
    }
);


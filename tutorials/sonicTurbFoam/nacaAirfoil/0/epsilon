FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          epsilon;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 25000;

boundaryField
{
    INLE1           
    {
        type            fixedValue;
        value           uniform 25000;
    }

    OUTL2           
    {
        type            zeroGradient;
    }

    SYMP3           
    {
        type            empty;
    }

    WALL10          
    {
        type            zeroGradient;
    }
}



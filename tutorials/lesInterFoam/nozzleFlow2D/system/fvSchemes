FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    div(phi,U)      Gauss localBlended Phi 1 Gamma2V 1;
    div(phi,gamma)  Gauss Gamma201 0.2;
    div(phirb,gamma) Gauss Gamma201 0.2;
    div(phi,k)      Gauss Gamma2 1;
    div(phi,B)      Gauss Gamma2 0.1;
    div(B)          Gauss linear;
    div(phi,nuTilda) Gauss Gamma2 0.1;
    div((nuEff*dev(grad(U).T()))) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear limited 0.3333;
}

interpolationSchemes
{
    default         linear;
    interpolate(HbyA) linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    pd;
    pcorr;
    gamma;
}



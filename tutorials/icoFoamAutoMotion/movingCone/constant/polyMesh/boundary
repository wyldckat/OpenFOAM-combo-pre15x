{
    type patch;
    physicalType wall;
    nFaces 50;
    startFace 3750;
}

fixedWall
{
    type wall;
    physicalType wall;
    nFaces 35;
    startFace 3800;
}

axis
{
    type empty;
    physicalType empty;
    nFaces 0;
    startFace 3835;
}

left
{
    type patch;
    physicalType atmosphere;
    nFaces 30;
    startFace 3835;
}

farField
{

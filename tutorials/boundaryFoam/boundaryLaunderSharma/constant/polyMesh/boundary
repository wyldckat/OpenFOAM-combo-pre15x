FoamFile
{
    version 2.0;
    format ascii;

    root "boundaryFoam";
    case "boundaryLaunderSharma";
    instance ""constant"";
    local "polyMesh";

    class polyBoundaryMesh;
    object boundary;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
lowerWall
{
    type wall;
    physicalType wall;
    nFaces 1;
    startFace 79;
}

upperWall
{
    type wall;
    physicalType wall;
    nFaces 1;
    startFace 80;
}

defaultFaces
{
    type empty;
    nFaces 320;
    startFace 81;
}
)


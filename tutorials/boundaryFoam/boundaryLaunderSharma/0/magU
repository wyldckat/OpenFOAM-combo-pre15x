FoamFile
{
    version 2.0;
    format ascii;

    root "boundaryFoam";
    case "boundaryLaunderSharma";
    instance ""0"";
    local "";

    class volScalarField;
    object magU;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lowerWall
    {
        type            calculated;
        value           uniform 0;
    }
    upperWall
    {
        type            calculated;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}



{
    version 2.0;
    format ascii;

    root "/home/dm2/henry/foam/henry2.3/run/tutorials/turbFoam";
    case "cavity";
    instance "constant";
    local "polyMesh";

    class polyBoundaryMesh;
    object boundary;
}


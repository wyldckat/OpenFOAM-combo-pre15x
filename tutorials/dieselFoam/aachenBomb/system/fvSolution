FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    rho             ICCG 1e-06 0;
    U               BICCG 1e-06 0;
    p               ICCG 1e-09 0;
    Yi              BICCG 1e-06 0;
    h               BICCG 1e-06 0;
    k               BICCG 1e-06 0;
    epsilon         BICCG 1e-06 0;
}

PISO
{
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
}



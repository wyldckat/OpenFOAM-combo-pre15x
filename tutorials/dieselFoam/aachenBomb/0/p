version             1.0;
format              ascii;
root                "";
case                "example";
instance            "0";
local               "";
class               volScalarField;
form                dictionary;
object              p;

// ************************************************************************* //

dimensions          [ 1 -1 -2 0 0 0 0 ];

internalField       uniform 5e+06;

referenceLevel      0;

boundaryField
{
    walls
    {
        type                zeroGradient;
    }
	
	front
	{
		type				wedge;
	}

	back
	{
		type				wedge;
	}

}

